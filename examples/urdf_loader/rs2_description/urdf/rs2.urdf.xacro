<?xml version="1.0" ?>
<robot name="rs2" xmlns:xacro="http://ros.org/wiki/xacro">
    <xacro:macro name="rs2" params="parent *origin name:=rs2">

        <material name="black">
            <color rgba="0.3 0.3 0.3 1.0"/>
        </material>

        <joint name="${name}_joint" type="fixed">
          <xacro:insert_block name="origin" />
          <parent link="${parent}"/>
          <child link="${name}_base_link" />
        </joint>

        <link name="${name}_base_link">
            <visual>
                <geometry>
                    <mesh filename="package://rs2_description/meshes/rs2/Base_jnt_def.stl"/>
                </geometry>
                <material name="black"/>
            </visual>
            <collision>
                <origin xyz="0.055 0 0.04156"/>
                <geometry>
                    <box size="0.112 0.110 0.08312"/>
                </geometry>
            </collision>
        </link>

        <link name="${name}_1013_base">
            <visual>
                <geometry>
                    <mesh filename="package://rs2_description/meshes/rs2/1013_base_def.stl"/>
                </geometry>
                <material name="black"/>
            </visual>
            <collision>
                <origin xyz="0 0 0.0316"/>
                <geometry>
                    <cylinder radius="0.046" length="0.0532"/>
                </geometry>
            </collision>
            <collision>
                <origin xyz="-0.046 0 0.0632" rpy="1.5707 0 0"/>
                <geometry>
                    <cylinder radius="0.046" length="0.092"/>
                </geometry>
            </collision>
        </link>

        <link name="${name}_1013_to_1015">
            <visual>
                <geometry>
                    <mesh filename="package://rs2_description/meshes/rs2/1013_to_1015_def.stl"/>
                </geometry>
                <material name="black"/>
            </visual>
            <visual>
                <material name="orange">
                    <color rgba="1.0 0.5 0.0 1"/>
                </material>
                <geometry>
                    <mesh filename="package://rs2_description/meshes/rs2/1013_to_1015_def_pods.stl"/>
                </geometry>
            </visual>

            <collision>
                <origin xyz="-0.046 0 -0.145275" rpy="0 0 0"/>
                <geometry>
                    <cylinder radius="0.046" length="0.28055"/>
                </geometry>
            </collision>

            <collision>
                <origin xyz="-0.0052 0 -0.29055" rpy="1.5707 0 0"/>
                <geometry>
                    <cylinder radius="0.041" length="0.092"/>
                </geometry>
            </collision>
        </link>

        <link name="${name}_1015_Elbow">
            <visual>
                <geometry>
                    <mesh filename="package://rs2_description/meshes/rs2/1015_Elbow_assem_def.stl"/>
                </geometry>
                <material name="black"/>
            </visual>
            <collision>
                <origin xyz="0.0402 0 0.048255" rpy="0 0 0"/>
                <geometry>
                    <cylinder radius="0.041" length="0.08645"/>
                </geometry>
            </collision>
        </link>

        <link name="${name}_1015_to_wrist">

            <visual>
                <geometry>
                    <mesh filename="package://rs2_description/meshes/rs2/1015_to_wrist_assem_def.stl"/>
                </geometry>
                <material name="black"/>
            </visual>
            <visual>
                <geometry>
                    <mesh filename="package://rs2_description/meshes/rs2/1015_to_wrist_assem_def_pods.stl"/>
                </geometry>
                <material name="orange">
                    <color rgba="1.0 0.5 0.0 1"/>
                </material>
            </visual>
            <collision>
                <origin xyz="0.00 0 0.05712" rpy="0 0 0"/>
                <geometry>
                    <cylinder radius="0.041" length="0.10424"/>
                </geometry>
            </collision>
            <collision>
                <origin xyz="0.0402 0 0.11424" rpy="1.5707 0 0"/>
                <geometry>
                    <cylinder radius="0.041" length="0.092"/>
                </geometry>
            </collision>
        </link>

        <link name="${name}_wrist">
            <visual>
                <geometry>
                    <mesh filename="package://rs2_description/meshes/rs2/wrist_assem_def.stl"/>
                </geometry>
                <material name="black"/>
            </visual>
            <collision>
                <origin xyz="0.0402 0 -0.044315" rpy="0 0 0"/>
                <geometry>
                    <cylinder radius="0.041" length="0.07863"/>
                </geometry>
            </collision>
        </link>

        <link name="${name}_gripper">
            <visual>
                <geometry>
                    <mesh filename="package://rs2_description/meshes/rs2/Gripper_def.stl"/>
                </geometry>
                <material name="black"/>
            </visual>
            <visual>
                <geometry>
                    <mesh filename="package://rs2_description/meshes/rs2/Gripper_def_pods.stl"/>
                </geometry>
                <material name="orange">
                    <color rgba="1.0 0.5 0.0 1"/>
                </material>
            </visual>
            <collision>
                <origin xyz="0.0 0 -0.059" rpy="0 0 0"/>
                <geometry>
                    <cylinder radius="0.041" length="0.108"/>
                </geometry>
            </collision>

            <collision>
                <origin xyz="0.0 0 -0.188" rpy="0 0 0"/>
                <geometry>
                    <box size="0.1 0.09 0.14"/>
                </geometry>
            </collision>

            <collision>
                <origin xyz="-0.033 0 -0.188" rpy="0 0.40 0"/>
                <geometry>
                    <box size="0.05 0.09 0.14"/>
                </geometry>
            </collision>
            <collision>
                <origin xyz="0.033 0 -0.188" rpy="0 -0.40 0"/>
                <geometry>
                    <box size="0.05 0.09 0.14"/>
                </geometry>
            </collision>
        </link>

        <link name="${name}_RS2_255_1">
            <visual>
                <geometry>
                    <mesh filename="package://rs2_description/meshes/rs2/RS2-255-def.stl"/>
                </geometry>
                <material name="black"/>
            </visual>
        </link>

        <link name="${name}_RS2_255_2">
            <visual>
                <geometry>
                    <mesh filename="package://rs2_description/meshes/rs2/RS2-255-def_jaw_inside.stl"/>
                </geometry>
                <material name="black"/>
            </visual>
        </link>

        <link name="${name}_push_rod"/>

        <joint name="${name}_push_rod_joint" type="prismatic">
            <origin xyz="0 0 -0.118" rpy="0 0 0"/>
            <parent link="${name}_gripper"/>
            <child link="${name}_push_rod"/>
            <axis xyz="0 0 1"/>
            <limit lower="0" upper="0.035" effort="10" velocity="10"/>
        </joint>


        <joint name="${name}_gripper_joint_1" type="revolute">
            <parent link="${name}_gripper"/>
            <child link="${name}_RS2_255_2"/>
            <origin xyz="-0.033 0 -0.118" rpy="0 0 0"/>
            <axis xyz="0 1 0"/>
            <limit effort="9.0" lower="0.0" upper="0.5" velocity="0.5"/>
        </joint>

        <joint name="${name}_gripper_joint_2" type="revolute">
            <parent link="${name}_gripper"/>
            <child link="${name}_RS2_255_1"/>
            <origin xyz="0.033 0 -0.118" rpy="0 0 0"/>
            <axis xyz="0 -1 0"/>
            <limit effort="9.0" lower="0.0" upper="0.5" velocity="0.5"/>
            <mimic joint="${name}_gripper_joint_1" multiplier="1"
            offset="0"/>
        </joint>


        <joint name="${name}_joint_b" type="revolute">
            <parent link="${name}_wrist"/>
            <child link="${name}_gripper"/>
            <origin xyz="0.040 0 -0.08863" rpy="0 0 0"/>
            <axis xyz="0 0 1"/>
            <limit effort="9.0" lower="0.0" upper="6.3" velocity="0.5"/>
        </joint>

        <joint name="${name}_joint_c" type="revolute">
            <parent link="${name}_1015_to_wrist"/>
            <child link="${name}_wrist"/>
            <origin xyz="0.040 0 0.11424" rpy="0 0 0"/>
            <axis xyz="0 -1 0"/>
            <limit effort="9.0" lower="0.0" upper="3.2" velocity="0.5"/>
        </joint>


        <joint name="${name}_joint_d" type="revolute">
            <parent link="${name}_1015_Elbow"/>
            <child link="${name}_1015_to_wrist"/>
            <origin xyz="0.040 0 0.09645" rpy="0 0 0"/>
            <axis xyz="0 0 -1"/>
            <limit effort="9.0" lower="0.0" upper="6.3" velocity="0.5"/>
        </joint>

        <joint name="${name}_joint_e" type="revolute">
            <parent link="${name}_1013_to_1015"/>
            <child link="${name}_1015_Elbow"/>
            <origin xyz="-0.0052 0 -0.29055" rpy="0 0 0"/>
            <axis xyz="0 1 0"/>
            <limit effort="9.0" lower="0.0" upper="3.2" velocity="0.5"/>
        </joint>

        <joint name="${name}_joint_f" type="revolute">
            <parent link="${name}_1013_base"/>
            <child link="${name}_1013_to_1015"/>
            <origin xyz="-0.046 0 0.0632" rpy="0 0 0"/>
            <axis xyz="0 1 0"/>
            <limit effort="9.0" lower="0.0" upper="3.2" velocity="0.5"/>
        </joint>


        <joint name="${name}_joint_g" type="revolute">
            <parent link="${name}_base_link"/>
            <child link="${name}_1013_base"/>
            <origin xyz="0.0665 0 0.08312" rpy="0 0 0"/>
            <axis xyz="0 0 1"/>
            <limit effort="9.0" lower="0.0" upper="6.2" velocity="0.5"/>
        </joint>
    </xacro:macro>
</robot>
